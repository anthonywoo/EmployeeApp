<%= form_for @team do |f| %>
  <%= f.label :name, "Name:" %> 
  <%= f.text_field :name %>
  <br>
  <%= "Members:" %>
  <% Employee.all.each do |employee| %>
    <label class="checkbox"> <%= employee.name %>
    <%= check_box_tag "team[member_ids][]", employee.id, @team.member_ids.include?(employee.id), :id=>"emp_id#{employee.id}" %>
    </label>
  <% end %>
  <%= hidden_field_tag "team[member_ids][]", "" %>
  <br>
  <%= f.submit %>
<% end %>
